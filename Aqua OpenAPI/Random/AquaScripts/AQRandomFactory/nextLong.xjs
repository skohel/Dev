print("Generate a random long:");
for ( i = 0; i < 5; i++ ) {
    var x = aqua.random.nextLong();
    print("\t"+x);
}

var min = -1000;
var max = 1000;
print("Generate a random long between " + min + " and " + max + ":");
for ( i = 0; i < 5; i++ ) {
    var x = aqua.random.nextLong(min, max);
    print("\t"+x);
}

