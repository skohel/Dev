print("Generate a random float:");
for ( i = 0; i < 5; i++ ) {
    var x = aqua.random.nextFloat();
    print("\t"+x);
}

var min = -1000;
var max = 1000;
print("Generate a random float between " + min + " and " + max + ":");
for ( i = 0; i < 5; i++ ) {
    var x = aqua.random.nextFloat(min, max);
    print("\t"+x);
}

