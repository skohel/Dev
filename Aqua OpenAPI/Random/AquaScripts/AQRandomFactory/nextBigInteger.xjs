print("Generate a random big integer:");
for ( i = 0; i < 5; i++ ) {
    var x = aqua.random.nextBigInteger();
    print("\t"+x);
}

var min = -1000;
var max = 1000;
print("Generate a random big integer between " + min + " and " + max + ":");
for ( i = 0; i < 5; i++ ) {
    var x = aqua.random.nextBigInteger(min, max);
    print("\t"+x);
}

