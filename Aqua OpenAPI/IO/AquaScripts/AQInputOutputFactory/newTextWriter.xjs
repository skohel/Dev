var textFile = "C:/text.out";
var str      = "Aquafold Inc.";

var writer = aqua.io.newTextWriter();
writer.setFile(textFile);
writer.write(str);
writer.close();
readFile();


function readFile() {
    var reader = aqua.io.newTextReader();
    reader.setFile(textFile);
    var row = null;
    while((row = reader.readNextRow()) != null) {
        print(row);
    }
    print();
}
