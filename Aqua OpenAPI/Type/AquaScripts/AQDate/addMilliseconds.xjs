var factory = aqua.type.getDate();
var date    = factory.newDate();
var unit    = 1;

aqua.console.println("Current datetime:                       [" + date + "]");

aqua.console.println("Current datetime + [" + unit + "] millisecond(s):  [" + date.addMilliseconds(unit) + "]");

unit = -1;
aqua.console.println("Current datetime + [" + unit + "] millisecond(s): [" + date.addMilliseconds(unit) + "]");