aqua.console.println("Is [" + value1 + "] equal to [" + value2 + "]? " + factory.isEqual(value1, value2));

value2 = 200000;
aqua.console.println("Is [" + value1 + "] equal to [" + value2 + "]? " + factory.isEqual(value1, value2));

