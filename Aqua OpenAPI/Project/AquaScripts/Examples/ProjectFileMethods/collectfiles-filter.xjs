ap = aqua.project;

objFile = ap.getAquaScript("");

collect = objFile.collectFiles("*User*.xjs");

for(i=0;i<collect.length;i++) {
    print(collect[i].getName());
}
function print(m) {
    aqua.console.println(m);
}