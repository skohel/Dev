var session = aqua.request.getSession();
aqua.console.println("calling aqua.session.setAttribute(\"key\", \"value\")");
session.setAttribute("key", "value");
aqua.console.println("aqua.session.getAttribute(\"key\"): [" + session.getAttribute("key") + "]\n");

aqua.console.println("calling aqua.session.removeAttribute(\"key\")");
session.removeAttribute("key");
aqua.console.println("aqua.session.getAttribute(\"key\"): [" + session.getAttribute("key") + "]");
