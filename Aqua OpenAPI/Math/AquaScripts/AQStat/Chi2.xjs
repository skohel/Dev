

var observed=new Array();
var expected=new Array();
var out=0;

//generate data
for(i=0;i<30;i++){

   observed[i]=100+Math.round(20*Math.random()-10); //number of heads observed in 200 coin flips
   expected[i]=100;  //number of heads expected in 200 coin flips
}

out=aqua.math.stat.Chi2(observed,expected);

aqua.console.println("Chi-Squared value = " + out);



